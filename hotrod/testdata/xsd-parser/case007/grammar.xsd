<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <!-- 7. No Attributes, Elements, Text -->

  <xsd:element name="foo">
    <xsd:complexType mixed="true">
      <xsd:sequence>
        <xsd:element name="bar" type="xsd:string" minOccurs="0"
          maxOccurs="unbounded" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

</xsd:schema>