<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.dates">

  <!-- columns -->

  <sql id="columns">
    "ID",
    "DAT1",
    "DAT2",
    "DAT3",
    "DAT4"
  </sql>

  <sql id="columnsWithAlias">
    ${alias}."ID",
    ${alias}."DAT1",
    ${alias}."DAT2",
    ${alias}."DAT3",
    ${alias}."DAT4"
  </sql>

  <resultMap id="allColumns" type="app.daos.DatesVO">
    <id property="id" column="ID" />
    <result property="dat1" column="DAT1" />
    <result property="dat2" column="DAT2" />
    <result property="dat3" column="DAT3" />
    <result property="dat4" column="DAT4" />
  </resultMap>

  <!-- select by PK -->

  <select id="selectByPK" resultMap="allColumns">
    select
      <include refid="columns" />
     from dates
     where
      "ID" = #{id,jdbcType=NUMERIC}
  </select>

  <!-- no select by unique indexes since there are no UI (besides the PK) -->

  <!-- select by example -->

  <select id="selectByExample" resultMap="allColumns">
    select
      <include refid="columns" />
     from dates
    <trim prefix="where&#xA;" prefixOverrides="and |or ">
      <if test="p.id != null">
        and "ID" = #{p.id,jdbcType=NUMERIC}
      </if>
      <if test="p.dat1 != null">
        and "DAT1" = #{p.dat1,jdbcType=TIMESTAMP}
      </if>
      <if test="p.dat2 != null">
        and "DAT2" = #{p.dat2,jdbcType=TIMESTAMP}
      </if>
      <if test="p.dat3 != null">
        and "DAT3" = #{p.dat3,jdbcType=TIMESTAMP}
      </if>
      <if test="p.dat4 != null">
        and "DAT4" = #{p.dat4,jdbcType=TIMESTAMP}
      </if>
    </trim>
    <if test="o != null">
      order by ${o}
    </if>
  </select>

  <!-- select by criteria -->

  <select id="selectByCriteria" resultMap="allColumns">
    ${sql}
  </select>

  <!-- insert (no auto-generated columns) -->

  <insert id="insert">
    insert into dates (
      <trim prefixOverrides=", ">
        <if test="true">, "ID"</if>
        <if test="true">, "DAT1"</if>
        <if test="true">, "DAT2"</if>
        <if test="true">, "DAT3"</if>
        <if test="true">, "DAT4"</if>
      </trim>
      ) values (
      <trim prefixOverrides=", ">
        <if test="true">, #{id,jdbcType=NUMERIC}</if>
        <if test="true">, #{dat1,jdbcType=TIMESTAMP}</if>
        <if test="true">, #{dat2,jdbcType=TIMESTAMP}</if>
        <if test="true">, #{dat3,jdbcType=TIMESTAMP}</if>
        <if test="true">, #{dat4,jdbcType=TIMESTAMP}</if>
      </trim>
      )
  </insert>

  <!-- update by PK -->

  <update id="updateByPK">
    update dates set
      "DAT1" = #{dat1,jdbcType=TIMESTAMP},
      "DAT2" = #{dat2,jdbcType=TIMESTAMP},
      "DAT3" = #{dat3,jdbcType=TIMESTAMP},
      "DAT4" = #{dat4,jdbcType=TIMESTAMP}
     where
      "ID" = #{id,jdbcType=NUMERIC}
  </update>

  <!-- delete by PK -->

  <delete id="deleteByPK">
    delete from dates
     where
      "ID" = #{id,jdbcType=NUMERIC}
  </delete>

  <!-- update by example -->

  <update id="updateByExample">
    update dates
    <set>
      <if test="values.id != null">"ID" = #{values.id,jdbcType=NUMERIC},</if>
      <if test="values.dat1 != null">"DAT1" = #{values.dat1,jdbcType=TIMESTAMP},</if>
      <if test="values.dat2 != null">"DAT2" = #{values.dat2,jdbcType=TIMESTAMP},</if>
      <if test="values.dat3 != null">"DAT3" = #{values.dat3,jdbcType=TIMESTAMP},</if>
      <if test="values.dat4 != null">"DAT4" = #{values.dat4,jdbcType=TIMESTAMP},</if>
    </set>
    <trim prefix="where&#xA;" prefixOverrides="and |or ">
      <if test="filter.id != null">
        and "ID" = #{filter.id,jdbcType=NUMERIC}
      </if>
      <if test="filter.dat1 != null">
        and "DAT1" = #{filter.dat1,jdbcType=TIMESTAMP}
      </if>
      <if test="filter.dat2 != null">
        and "DAT2" = #{filter.dat2,jdbcType=TIMESTAMP}
      </if>
      <if test="filter.dat3 != null">
        and "DAT3" = #{filter.dat3,jdbcType=TIMESTAMP}
      </if>
      <if test="filter.dat4 != null">
        and "DAT4" = #{filter.dat4,jdbcType=TIMESTAMP}
      </if>
    </trim>
  </update>

  <!-- update by criteria -->

  <select id="updateByCriteria">
    ${sql}
  </select>

  <!-- delete by example -->

  <delete id="deleteByExample">
    delete from dates
    <trim prefix="where&#xA;" prefixOverrides="and |or ">
      <if test="id != null">
        and "ID" = #{id,jdbcType=NUMERIC}
      </if>
      <if test="dat1 != null">
        and "DAT1" = #{dat1,jdbcType=TIMESTAMP}
      </if>
      <if test="dat2 != null">
        and "DAT2" = #{dat2,jdbcType=TIMESTAMP}
      </if>
      <if test="dat3 != null">
        and "DAT3" = #{dat3,jdbcType=TIMESTAMP}
      </if>
      <if test="dat4 != null">
        and "DAT4" = #{dat4,jdbcType=TIMESTAMP}
      </if>
    </trim>
  </delete>

  <!-- delete by criteria -->

  <select id="deleteByCriteria">
    ${sql}
  </select>

</mapper>
