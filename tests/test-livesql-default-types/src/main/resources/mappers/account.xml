      <if test="values.paymentDate != null">"payment_date" = #{values.paymentDate,jdbcType=DATE},</if>
      <if test="values.paymentAmount != null">"payment_amount" = #{values.paymentAmount,jdbcType=DOUBLE},</if>
    </set>
    <trim prefix="where&#xA;" prefixOverrides="and |or ">
      <if test="filter.paymentNumber != null">
        and "payment_number" = #{filter.paymentNumber,jdbcType=DOUBLE}
      </if>
      <if test="filter.dateGranted != null">
        and "date_granted" = #{filter.dateGranted,jdbcType=DATE}
      </if>
      <if test="filter.amountGranted != null">
        and "amount_granted" = #{filter.amountGranted,jdbcType=DOUBLE}
      </if>
      <if test="filter.interestRate != null">
        and "interest_rate" = #{filter.interestRate,jdbcType=DOUBLE}
      </if>
      <if test="filter.paymentDate != null">
        and "payment_date" = #{filter.paymentDate,jdbcType=DATE}
      </if>
      <if test="filter.paymentAmount != null">
        and "payment_amount" = #{filter.paymentAmount,jdbcType=DOUBLE}
      </if>
    </trim>
  </update>

