  <!-- select by PK: no mapping generated, since the table does not have a PK -->

  <!-- select by unique indexes: no mappings generated, since the table does not have UIs -->

  <!-- select by example -->

  <select id="selectByExample" resultMap="allColumns">
    select
      <include refid="columns" />
     from account
    <trim prefix="where&#xA;" prefixOverrides="and |or ">
      <if test="p.paymentNumber != null">
        and "payment_number" = #{p.paymentNumber,jdbcType=DOUBLE}
      </if>
      <if test="p.dateGranted != null">
        and "date_granted" = #{p.dateGranted,jdbcType=DATE}
      </if>
      <if test="p.amountGranted != null">
        and "amount_granted" = #{p.amountGranted,jdbcType=DOUBLE}
      </if>
      <if test="p.interestRate != null">
        and "interest_rate" = #{p.interestRate,jdbcType=DOUBLE}
      </if>
      <if test="p.paymentDate != null">
        and "payment_date" = #{p.paymentDate,jdbcType=DATE}
      </if>
      <if test="p.paymentAmount != null">
        and "payment_amount" = #{p.paymentAmount,jdbcType=DOUBLE}
      </if>
    </trim>
    <if test="o != null">
      order by ${o}
    </if>
  </select>

  <!-- select by criteria -->

  <select id="selectByCriteria" resultMap="allColumns">
    ${sql}
  </select>

  <!-- insert (no auto-generated columns) -->

  <insert id="insert">
    insert into account (
      <trim prefixOverrides=", ">
        <if test="true">, "payment_number"</if>
        <if test="true">, "date_granted"</if>
        <if test="true">, "amount_granted"</if>
        <if test="true">, "interest_rate"</if>
        <if test="true">, "payment_date"</if>
        <if test="true">, "payment_amount"</if>
      </trim>
      ) values (
      <trim prefixOverrides=", ">
        <if test="true">, #{paymentNumber,jdbcType=DOUBLE}</if>
        <if test="true">, #{dateGranted,jdbcType=DATE}</if>
        <if test="true">, #{amountGranted,jdbcType=DOUBLE}</if>
        <if test="true">, #{interestRate,jdbcType=DOUBLE}</if>
        <if test="true">, #{paymentDate,jdbcType=DATE}</if>
        <if test="true">, #{paymentAmount,jdbcType=DOUBLE}</if>
      </trim>
      )
  </insert>

  <!-- update by PK: no update mapping generated, since the table does not have a PK -->

  <!-- delete by PK: no delete mapping generated, since the table does not have a PK -->

  <!-- update by example -->

  <update id="updateByExample">
    update account
    <set>
      <if test="values.paymentNumber != null">"payment_number" = #{values.paymentNumber,jdbcType=DOUBLE},</if>
      <if test="values.dateGranted != null">"date_granted" = #{values.dateGranted,jdbcType=DATE},</if>
      <if test="values.amountGranted != null">"amount_granted" = #{values.amountGranted,jdbcType=DOUBLE},</if>
