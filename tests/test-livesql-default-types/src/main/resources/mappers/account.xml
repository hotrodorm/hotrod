  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.account">

  <!-- columns -->

  <sql id="columns">
    "payment_number",
    "date_granted",
    "amount_granted",
    "interest_rate",
    "payment_date",
    "payment_amount"
  </sql>

  <sql id="columnsWithAlias">
    ${alias}."payment_number",
    ${alias}."date_granted",
    ${alias}."amount_granted",
    ${alias}."interest_rate",
    ${alias}."payment_date",
    ${alias}."payment_amount"
  </sql>

  <resultMap id="allColumns" type="app.daos.AccountVO">
    <result property="paymentNumber" column="payment_number" />
    <result property="dateGranted" column="date_granted" />
    <result property="amountGranted" column="amount_granted" />
    <result property="interestRate" column="interest_rate" />
    <result property="paymentDate" column="payment_date" />
    <result property="paymentAmount" column="payment_amount" />
