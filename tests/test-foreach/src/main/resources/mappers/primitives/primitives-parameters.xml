<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.parameters">

  <!-- columns -->

  <sql id="columns">
    system,
    level,
    name,
    value
  </sql>

  <sql id="columnsWithAlias">
    ${alias}.system,
    ${alias}.level,
    ${alias}.name,
    ${alias}.value
  </sql>

  <resultMap id="allColumns" type="app.daos.ParametersVO">
    <result property="system" column="system" />
    <result property="level" column="level" />
    <result property="name" column="name" />
    <result property="value" column="value" />
  </resultMap>

  <!-- select by PK: no mapping generated, since the table does not have a PK -->

  <!-- select by unique indexes: no mappings generated, since the table does not have UIs -->

  <!-- select by example -->

  <select id="selectByExample" resultMap="allColumns">
    select
      <include refid="columns" />
     from public.parameters
    <trim prefix="where&#xA;" prefixOverrides="and |or ">
      <if test="p.system != null">
        and system = #{p.system,jdbcType=INTEGER}
      </if>
      <if test="p.level != null">
        and level = #{p.level,jdbcType=INTEGER}
      </if>
      <if test="p.name != null">
        and name = #{p.name,jdbcType=VARCHAR}
      </if>
      <if test="p.value != null">
        and value = #{p.value,jdbcType=VARCHAR}
      </if>
    </trim>
    <if test="o != null">
      order by ${o}
    </if>
  </select>

  <!-- select by criteria -->

  <select id="selectByCriteria" resultMap="allColumns">
    ${sql}
  </select>

  <!-- insert (no auto-generated columns) -->

  <insert id="insert">
    insert into public.parameters (
      <trim prefixOverrides=", ">
        <if test="true">, system</if>
        <if test="true">, level</if>
        <if test="true">, name</if>
        <if test="true">, value</if>
      </trim>
      ) values (
      <trim prefixOverrides=", ">
        <if test="true">, #{system,jdbcType=INTEGER}</if>
        <if test="true">, #{level,jdbcType=INTEGER}</if>
        <if test="true">, #{name,jdbcType=VARCHAR}</if>
        <if test="true">, #{value,jdbcType=VARCHAR}</if>
      </trim>
      )
  </insert>

  <!-- update by PK: no update mapping generated, since the table does not have a PK -->

  <!-- delete by PK: no delete mapping generated, since the table does not have a PK -->

  <!-- update by example -->

  <update id="updateByExample">
    update public.parameters
    <set>
      <if test="values.system != null">system = #{values.system,jdbcType=INTEGER},</if>
      <if test="values.level != null">level = #{values.level,jdbcType=INTEGER},</if>
      <if test="values.name != null">name = #{values.name,jdbcType=VARCHAR},</if>
      <if test="values.value != null">value = #{values.value,jdbcType=VARCHAR},</if>
    </set>
    <trim prefix="where&#xA;" prefixOverrides="and |or ">
      <if test="filter.system != null">
        and system = #{filter.system,jdbcType=INTEGER}
      </if>
      <if test="filter.level != null">
        and level = #{filter.level,jdbcType=INTEGER}
      </if>
      <if test="filter.name != null">
        and name = #{filter.name,jdbcType=VARCHAR}
      </if>
      <if test="filter.value != null">
        and value = #{filter.value,jdbcType=VARCHAR}
      </if>
    </trim>
  </update>

  <!-- update by criteria -->

  <select id="updateByCriteria">
    ${sql}
  </select>

  <!-- delete by example -->

  <delete id="deleteByExample">
    delete from public.parameters
    <trim prefix="where&#xA;" prefixOverrides="and |or ">
      <if test="system != null">
        and system = #{system,jdbcType=INTEGER}
      </if>
      <if test="level != null">
        and level = #{level,jdbcType=INTEGER}
      </if>
      <if test="name != null">
        and name = #{name,jdbcType=VARCHAR}
      </if>
      <if test="value != null">
        and value = #{value,jdbcType=VARCHAR}
      </if>
    </trim>
  </delete>

  <!-- delete by criteria -->

  <select id="deleteByCriteria">
    ${sql}
  </select>

</mapper>
