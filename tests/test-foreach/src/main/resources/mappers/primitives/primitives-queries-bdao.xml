<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.queriesBDAO">

  <!-- columns -->

  <sql id="columns">
    
  </sql>

  <sql id="columnsWithAlias">
    
  </sql>

  <!-- select method: findLowBranches -->

  <resultMap id="result_map_select_findLowBranches" type="app.daos.LowBranchVO">
    <result property="branchId" column="BRANCH_ID" />
    <result property="branchName" column="BRANCH_NAME" />
  </resultMap>

  <select id="select_findLowBranches" resultMap="result_map_select_findLowBranches">
      select *
      from branch
      order by branch_name desc
      limit 2
  </select>

</mapper>
